{
	"configurations": [
	{
		"name": "Run Frankentar",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/build/frankentar",
		"args": [
			"read",
			"test.ftar",
			"LICENSE.md"
		],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
		    {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		    }
		]
	}
	]
}